<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.FriendRequestEntity">
        <table name="FriendRequest" schema="dbo" catalog="community"/>
        <attributes>
            <id name="friendRequestId">
                <column name="friendRequest_id" precision="-1"/>
            </id>
            <basic name="message">
                <column name="message" nullable="false" length="256"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

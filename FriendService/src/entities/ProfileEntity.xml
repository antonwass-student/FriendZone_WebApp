<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.ProfileEntity">
        <table name="Profile" schema="dbo" catalog="community"/>
        <attributes>
            <id name="profileId">
                <column name="profile_id" precision="-1"/>
            </id>
            <basic name="birthday">
                <column name="birthday" nullable="false" precision="-1"/>
            </basic>
            <basic name="description">
                <column name="description" nullable="false" length="1024"/>
            </basic>
            <basic name="picture">
                <column name="picture" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

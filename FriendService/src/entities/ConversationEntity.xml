<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.ConversationEntity">
        <table name="Conversation" schema="dbo" catalog="community"/>
        <attributes>
            <id name="conversationId">
                <column name="conversation_id" precision="-1"/>
            </id>
            <basic name="namn">
                <column name="namn" length="64"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

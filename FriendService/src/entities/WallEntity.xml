<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.WallEntity">
        <table name="Wall" schema="dbo" catalog="community"/>
        <attributes>
            <id name="wallId">
                <column name="wall_id" precision="-1"/>
            </id>
            <many-to-one name="userByOwner" optional="true" target-entity="entities.UserEntity">
                <join-column name="owner" referenced-column-name="user_id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>

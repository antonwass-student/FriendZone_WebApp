<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.FriendshipEntity">
        <table name="Friendship" schema="dbo" catalog="community"/>
        <attributes>
            <id name="friendshipId">
                <column name="friendship_id" precision="-1"/>
            </id>
            <basic name="started">
                <column name="started" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
